{
  "name": "bridit/http",
  "description": "Brid Modular Micro-Framework (Http)",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Siqueira",
      "email": "gus@brid-it.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "bridit/core": "^1.0",
    "guzzlehttp/psr7": "^1.8",
    "lcobucci/jwt": "^4.1",
    "php-di/slim-bridge": "^3.1",
    "slim/csrf": "^1.2",
    "slim/slim": "^4.0",
    "nyholm/psr7": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.88"
  },
  "autoload": {
    "psr-4": {
      "Brid\\Http\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
